{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": false,
    "jsx": "react",
    "experimentalDecorators": false,
    "emitDecoratorMetadata": false,
    "declaration": false,
    "removeComments": false,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "sourceMap": false,
    "inlineSourceMap": true,
    "strict": true,
    "skipLibCheck": true
  },
  "compileOnSave": true
}
