{
  "name": "rhmap-ts-cloud",
  "version": "0.1.0",
  "dependencies": {
    "@types/bluebird": "^3.0.33",
    "@types/body-parser": "0.0.33",
    "@types/cors": "0.0.33",
    "@types/es6-promise": "0.0.32",
    "@types/express": "^4.0.33",
    "@types/mocha": "^2.2.32",
    "@types/mongodb": "^2.1.33",
    "@types/morgan": "^1.7.32",
    "bluebird": "^3.3.5",
    "body-parser": "~1.10.2",
    "cookie-parser": "~1.3.3",
    "cors": "^2.8.1",
    "express": "~4.11.1",
    "fh-mbaas-api": "^5.14.1",
    "mocha": "^3.1.1",
    "mongodb": "~2.1.18",
    "morgan": "~1.5.1",
    "nyc": "^8.3.1",
    "source-map-support": "^0.4.3",
    "typescript": "^v2.0.3",
    "ts-node": "^1.3.0"
  },
  "devDependencies": {
    "mocha": "^2.1.0",
    "proxyquire": "0.5.3",
    "sinon": "^1.17.2",
    "del-cli": "^0.2.0",
    "nodemon": "^1.9.2"
  },
  "main": "lib/application.js",
  "scripts": {
    "compile": "node_modules/.bin/tsc --pretty",
    "clean": "node_modules/.bin/del lib/**/*.js && node_modules/.bin/del lib/**/*.js.map",
    "postinstall": "npm run compile",
    "start": "npm run compile && node lib/application.js",
    "test": "nyc --require ts-node/register ./node_modules/.bin/_mocha"
  },
  "license": "MIT",
  "nyc": {
    "include": [
      "lib/**/*.ts"
    ],
    "exclude": [
      "node_modules/"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
