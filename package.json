{
  "name": "rhmap-ts-cloud",
  "version": "0.1.0",
  "main": "lib/application.js",
  "scripts": {
    "compile": "node_modules/.bin/tsc --pretty",
    "clean": "del lib/*.js && del lib/**/*.js && del .nyc_output && del coverage",
    "postinstall": "npm run compile",
    "start": "npm run compile && env NODE_PATH='.' node lib/application.js",
    "test": "npm run compile && export NODE_PATH='.' && nyc --require ts-node/register ./node_modules/.bin/_mocha"
  },
  "dependencies": {
    "@types/bluebird": "3.5.15",
    "@types/body-parser": "0.0.33",
    "@types/chai": "3.5.2",
    "@types/cors": "0.0.33",
    "@types/express": "4.0.37",
    "@types/mocha": "2.2.43",
    "@types/mongodb": "2.2.13",
    "@types/morgan": "1.7.33",
    "@types/node": "6.0.89",
    "@types/proxyquire": "1.3.28",
    "@types/sinon": "1.16.36",
    "@types/supertest": "1.1.32",
    "bluebird": "3.5.1",
    "body-parser": "1.10.2",
    "cookie-parser": "~1.3.3",
    "cors": "2.8.4",
    "env-var": "2.4.3",
    "express": "4.11.2",
    "fh-mbaas-api": "5.14.2",
    "mongodb": "2.2.31",
    "morgan": "1.5.3",
    "source-map-support": "0.4.18",
    "typescript": "2.5.3"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "del-cli": "0.2.1",
    "mocha": "3.5.3",
    "nodemon": "^1.9.2",
    "nyc": "8.4.0",
    "proxyquire": "0.5.3",
    "sinon": "1.17.7",
    "supertest": "2.0.1",
    "ts-node": "1.7.3"
  },
  "license": "MIT",
  "nyc": {
    "include": [
      "lib/**/*.ts"
    ],
    "exclude": [
      "node_modules/"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
