{
  "name": "rhmap-ts-cloud",
  "version": "0.1.0",
  "main": "lib/application.js",
  "scripts": {
    "compile": "node_modules/.bin/tsc --pretty",
    "clean": "del lib/*.js && del lib/**/*.js && del coverage",
    "postinstall": "npm run compile",
    "start": "npm run compile && env NODE_PATH='.' node lib/application.js",
    "lint": "tslint --fix -c ./tslint.json lib/**/*.ts",
    "unit-test": "jest && node remap-coverage.js && del coverage-raw",
    "test": "npm run clean && npm run compile && npm run lint && npm run unit-test"
  },
  "dependencies": {
    "@types/bluebird": "3.5.15",
    "@types/body-parser": "0.0.33",
    "@types/cors": "0.0.33",
    "@types/express": "4.0.37",
    "@types/jest": "21.1.2",
    "@types/mongodb": "2.2.13",
    "@types/morgan": "1.7.33",
    "@types/node": "6.0.89",
    "@types/supertest-as-promised": "2.0.34",
    "bluebird": "3.5.1",
    "body-parser": "1.10.2",
    "cookie-parser": "~1.3.3",
    "cors": "2.8.4",
    "env-var": "2.4.3",
    "express": "4.11.2",
    "fh-mbaas-api": "5.14.2",
    "mongodb": "2.2.31",
    "morgan": "1.5.3",
    "source-map-support": "0.4.18",
    "typescript": "2.5.3"
  },
  "devDependencies": {
    "del-cli": "0.2.1",
    "jest": "20.0.4",
    "remap-istanbul": "0.9.5",
    "supertest-as-promised": "4.0.2",
    "ts-jest": "20.0.14",
    "ts-lint": "4.5.1",
    "ts-node": "3.3.0"
  },
  "license": "MIT"
}
