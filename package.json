{
  "name": "rhmap-ts-cloud",
  "version": "0.1.0",
  "main": "lib/application.js",
  "scripts": {
    "compile": "node_modules/.bin/tsc --pretty",
    "clean": "del lib/*.js && del lib/**/*.js && del .nyc_output && del coverage",
    "postinstall": "npm run compile",
    "start": "npm run compile && env NODE_PATH='.' node lib/application.js",
    "test": "npm run compile && export NODE_PATH='.' && nyc --require ts-node/register ./node_modules/.bin/_mocha"
  },
  "dependencies": {
    "@types/bluebird": "^3.0.33",
    "@types/body-parser": "0.0.33",
    "@types/chai": "^3.4.34",
    "@types/cors": "0.0.33",
    "@types/express": "^4.0.33",
    "@types/mocha": "^2.2.32",
    "@types/mongodb": "^2.1.33",
    "@types/morgan": "^1.7.32",
    "@types/node": "^4.0.30",
    "@types/proxyquire": "^1.3.27",
    "@types/sinon": "^1.16.31",
    "@types/supertest": "^1.1.31",
    "bluebird": "^3.3.5",
    "body-parser": "~1.10.2",
    "cookie-parser": "~1.3.3",
    "cors": "^2.8.1",
    "env-var": "^2.4.2",
    "express": "~4.11.1",
    "fh-mbaas-api": "^5.14.1",
    "mongodb": "^2.2.29",
    "morgan": "~1.5.1",
    "source-map-support": "^0.4.3",
    "typescript": "^2.3.2"
  },
  "devDependencies": {
    "ts-node": "^1.3.0",
    "chai": "^3.5.0",
    "mocha": "^3.1.1",
    "nyc": "^8.3.1",
    "proxyquire": "0.5.3",
    "sinon": "^1.17.2",
    "supertest": "^2.0.0",
    "del-cli": "^0.2.0",
    "nodemon": "^1.9.2"
  },
  "license": "MIT",
  "nyc": {
    "include": [
      "lib/**/*.ts"
    ],
    "exclude": [
      "node_modules/"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
